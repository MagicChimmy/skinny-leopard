<h1>Please fill in details of new listing:</h1>
<%= simple_form_for(@instrument) do |f| %>



  <%= f.input :name %>
  <%= f.input :photo %>
  <%= f.input :photo_cache, as: :hidden %>
  <%= f.input :category %>
  <%= f.input :description %>
  <%= f.input :price %>
  <%= f.input :location %>
  <button class="btn btn-primary"><%= f.button :submit %></button>

<% end %>

