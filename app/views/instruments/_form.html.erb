<%= simple_form_for(@instrument) do |f| %>
  <%= f.input :name %>
  <%= f.input :photo %>
  <%= f.input :photo_cache, as: :hidden %>
  <%= f.input :category, collection: ["Electronic", "Percussion", "Electronic", "Classical", "Strings", "Woodwind", "Brass"]  %>
  <%= f.input :price %>
  <%= f.input :location %>
  <%= f.input :description %>
  <button class="btn btn-flat yellow"><%= f.button :submit %></button>

<% end %>

